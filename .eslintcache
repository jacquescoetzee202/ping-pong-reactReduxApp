\n});\n\nconst player2 = ( state, action ) => ({\n  ...state, \n  player2: state.player2 + 1,\n  winner: action.winner,\n  servingP1: action.servingP1,\n});\n\nconst server = state => {\n  \n  const { \n    servingP1, \n    player1, \n    player2, \n    alternateAt, \n    winningScore \n  } = state;\n\n  let deuce = winningScore - 1;\n\n  let modulo = player1 >= deuce && player2 >= deuce ? 2 : alternateAt;\n  let serveChange = (player1 + player2) % modulo === 0;\n\n  return {\n    ...state,\n    servingP1: serveChange ? !servingP1 : servingP1,\n  }\n};\n\nconst winner = state => {\n  const { winner, player1, player2, winningScore } = state;\n\n  if (winner > 0 ) {\n    return { ...state };\n  } \n  else if ( player1 >= winningScore && player1 - player2 > 1 ) {\n    return { ...state, winner: 1 };\n  } \n  else if ( player2 >= winningScore && player2 - player1 > 1 ) {\n    return { ...state, winner: 2 };\n  } else {\n    return { ...state };\n  }\n}\n\nconst history = state => {\n  if(state.winner === 0) {\n    return state;\n  }\n\n  const { player1, player2, winner } = state;\n\n  return {\n    ...state,\n    gameHistory: [ \n      ...state.gameHistory,\n      {\n        player_1: {\n          score: player1,\n          won: winner === 1\n        },\n        player_2: {\n          score: player2,\n          won: winner === 2\n        },\n      }\n    ]\n  } \n}\n\nconst gameSettings = (state, action) => {\n\n  const { player1Name, player2Name, winningScore, alternateAt, id } = action;\n  \n  return {\n    ...state,\n    player1Name,\n    player2Name,\n    winningScore: +winningScore,\n    alternateAt: +alternateAt,\n    id: +id,\n    inProgress: !state.inProgress,\n  }\n}\n\nconst reset = state => {\n  return {\n  ...initialState, \n  gameHistory: state.gameHistory,\n  inProgress: !state.inProgress,\n  }\n}\n\n\n// Reducer\nconst reducer = (state, action) => {\n  switch (action.type) {\n    case \"PLAYER_1\": return history((player1(state, action)));\n    case \"PLAYER_2\": return history((player2(state, action)));\n    case \"RESET\": return reset(state);\n    case \"LANGUAGE_1\": return { ...state, language: 1 }\n    case \"LANGUAGE_2\": return { ...state, language: 2 }\n    case \"SAVE_SETTINGS\": return gameSettings(state, action);\n    default : return state;\n  }\n};\n\nexport default reducer;","/Users/jacquescoetzee/coding-fellowship/redux/ping-pong/src/data/store.js",[],"/Users/jacquescoetzee/coding-fellowship/redux/ping-pong/src/components/Results/index.js",[],"/Users/jacquescoetzee/coding-fellowship/redux/ping-pong/src/components/Results/ResultsTable.js",[],"/Users/jacquescoetzee/coding-fellowship/redux/ping-pong/src/components/WinMessage/index.js",[],"/Users/jacquescoetzee/coding-fellowship/redux/ping-pong/src/components/WinMessage/WinMessage.js",[],"/Users/jacquescoetzee/coding-fellowship/redux/ping-pong/src/components/Player/Player.js",[],"/Users/jacquescoetzee/coding-fellowship/redux/ping-pong/src/language/translate.js",[],"/Users/jacquescoetzee/coding-fellowship/redux/ping-pong/src/components/Reset/index.js",[],"/Users/jacquescoetzee/coding-fellowship/redux/ping-pong/src/components/Reset/Reset.js",[],"/Users/jacquescoetzee/coding-fellowship/redux/ping-pong/src/components/Header/index.js",[],"/Users/jacquescoetzee/coding-fellowship/redux/ping-pong/src/components/Header/Header.js",[],"/Users/jacquescoetzee/coding-fellowship/redux/ping-pong/src/components/Language/index.js",[],"/Users/jacquescoetzee/coding-fellowship/redux/ping-pong/src/components/Language/Language.js",[],"/Users/jacquescoetzee/coding-fellowship/redux/ping-pong/src/components/Player/Player2.js",[],"/Users/jacquescoetzee/coding-fellowship/redux/ping-pong/src/components/Player/Player1.js",[],"/Users/jacquescoetzee/coding-fellowship/redux/ping-pong/src/components/Player/Button.js",[],"/Users/jacquescoetzee/coding-fellowship/redux/ping-pong/src/components/Form/Form.js",[],"/Users/jacquescoetzee/coding-fellowship/redux/ping-pong/src/components/Form/index.js",["137"],"import Form from \"./Form\";\n\nimport { connect } from \"react-redux\";\nimport { saveSettings } from \"../../data/actions/state\";\nimport translate from \"../../language/translate\";\nimport { postGame } from \"../../data/actions/api\";\n\nconst mapStateToProps = state => {\n    \n    const { player1Name, player2Name, winningScore, alternateAt } = state;\n    \n    let wording = [ \"player 1 name\", \"player 2 name\", \"winning score\", \"alternate every\", \"start game\"];\n\n    let translated = wording.map( word => ( translate( state.language, word ) ) );\n\n    return {\n        player1Name,\n        player2Name,\n        winningScore,\n        alternateAt,\n        player1Txt: translated[0],\n        player2Txt: translated[1],\n        winScoreTxt: translated[2],\n        alternateTxt: translated[3],\n        startTxt: translated[4],\n    }\n}\n\n\nconst mapDispatchToProps = dispatch => {\n    return {\n        startGame: data => dispatch(postGame(data)),\n    }\n};\n\nexport default connect(mapStateToProps, mapDispatchToProps)(Form);","/Users/jacquescoetzee/coding-fellowship/redux/ping-pong/src/components/Loading/Loading.js",[],"/Users/jacquescoetzee/coding-fellowship/redux/ping-pong/src/components/Loading/index.js",[],"/Users/jacquescoetzee/coding-fellowship/redux/ping-pong/src/data/actions/state.js",[],"/Users/jacquescoetzee/coding-fellowship/redux/ping-pong/src/data/actions/api.js",["138"],"/Users/jacquescoetzee/coding-fellowship/redux/ping-pong/src/axios-config.js",[],"/Users/jacquescoetzee/coding-fellowship/redux/ping-pong/src/components/App.js",[],"/Users/jacquescoetzee/coding-fellowship/redux/ping-pong/src/components/Game/index.js",[],"/Users/jacquescoetzee/coding-fellowship/redux/ping-pong/src/components/Settings/index.js",[],"/Users/jacquescoetzee/coding-fellowship/redux/ping-pong/src/components/Settings/Settings.js",[],"/Users/jacquescoetzee/coding-fellowship/redux/ping-pong/src/components/Game/Game.js",[],{"ruleId":"139","replaced